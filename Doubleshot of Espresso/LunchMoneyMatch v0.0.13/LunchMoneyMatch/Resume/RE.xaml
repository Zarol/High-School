<UserControl x:Class="LunchMoneyMatch.Resume.RE"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" >
    <Expander Name="primaryExpander" Header="EMPTY" IsExpanded="True">
        <Grid Height="150" Width="500" Name="myGrid">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="1*" />
                <ColumnDefinition Width=".5*" />
            </Grid.ColumnDefinitions>
            <Border BorderBrush="#FFB3CCF0" BorderThickness="0,1,0,1" Margin="12,10,0,10" Name="innerBorder">
                <StackPanel Name="myContent" />
            </Border>
            <StackPanel Grid.Column="1" VerticalAlignment="Center" HorizontalAlignment="Center" Orientation="Horizontal">
                <Button Grid.Column="1" Width="75" Height="24" Content="Hide" Margin="5"/>
                <Button Grid.Column="1" Width="75" Height="24" Content="Remove" Margin="5"/>
            </StackPanel>
        </Grid>
    </Expander>
</UserControl>
